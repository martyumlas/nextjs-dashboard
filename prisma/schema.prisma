// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://root:password@postgres:5432/nextjs_dashboard_db"
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
  password String
}